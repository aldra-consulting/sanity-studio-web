{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@aldra-consulting/typescript-config/react",
  "compilerOptions": {
    "skipLibCheck": true,
    "jsx": "preserve",
    "paths": {
      "@project/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "files": [
    "sanity.cli.ts",
    "sanity.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
